{
    "name": "c0ntax/aws-ec2-check-tag",
    "description": "A very simple service that pulls the tags for a given instance to see if a particular key = a particular value.",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "Apache-2.0",
    "keywords": [
        "aws",
        "ec2",
        "tags",
        "instance"
    ],
    "authors": [
        {
            "name": "Dan Burzynski",
            "email": "contax@phrenetic.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "C0ntax\\Aws\\Ec2\\CheckTag\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "C0ntax\\Aws\\Ec2\\CheckTag\\Tests\\": "tests"
        }
    },
    "repositories": [
        {
            "type": "github",
            "url": "git@github.com:d9magai/Ec2MetadataGetter.git"
        }
    ],
    "require": {
        "php": "^7.4",
        "aws/aws-sdk-php": "^3.0",
        "razorpay/ec2-metadata": "dev-master#5be4661c3d3e672a862c84d8432a8862bd9d5e41"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    }
}
